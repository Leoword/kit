{
	"name": "@produck/kit",
	"version": "0.0.0",
	"description": "A useful dependency injection module.",
	"keywords": [
		"duck",
		"injection",
		"kit",
		"DI",
		"IoC"
	],
	"author": "ChaosLee <chaos2lee@qq.com>",
	"homepage": "https://github.com/produck/kit/tree/main/packages/kit#readme",
	"license": "MIT",
	"files": [
		"src",
		"dist/index.cjs",
		"index.d.ts",
		"tsconfig.json"
	],
	"types": "index.d.ts",
	"main": "dist/index.cjs",
	"type": "module",
	"exports": {
		"import": "./index.mjs",
		"require": "./dist/index.cjs"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/produck/kit.git"
	},
	"scripts": {
		"build": "rollup -c script/rollup.config.mjs",
		"lint": "eslint --fix **/*.mjs",
		"test": "mocha \"**/*-spec.mjs\" -t 999999999 ",
		"coverage": "c8 --reporter=lcov npm test",
		"prepublishOnly": "npm run lint && npm test && npm run build"
	},
	"bugs": {
		"url": "https://github.com/produck/kit/issues"
	},
	"devDependencies": {
		"c8": "^7.12.0",
		"eslint": "^8.24.0",
		"mocha": "^10.0.0",
		"rollup": "^2.79.1"
	}
}
